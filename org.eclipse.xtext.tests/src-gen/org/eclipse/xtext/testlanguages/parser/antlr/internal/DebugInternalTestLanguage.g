/*
 * generated by Xtext
 */
grammar DebugInternalTestLanguage ;

// Rule EntryRule
ruleEntryRule :
	ruleAbstractRule*
;

// Rule AbstractRule
ruleAbstractRule :
	ruleChoiceRule |
	ruleReducibleRule
;

// Rule ChoiceRule
ruleChoiceRule :
	'choice' 'optional'? RULE_ID
;

// Rule ReducibleRule
ruleReducibleRule :
	'reducible' ruleTerminalRule ruleTerminalRule?
;

// Rule TerminalRule
ruleTerminalRule :
	RULE_STRING
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;